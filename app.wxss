/**app.wxss**/

a,h1,h2,h3,img,body,html,ul,li,p{
  margin: 0;
  padding: 0;
}
 image {
  border:0;
  vertical-align: middle;
} 

.clearfix::before,
.clearfix:after {
    content:"";
    display:block;
    height:0;
    line-height: 0;
    visibility:hidden;
    clear:both;
}
.clearfix {
    zoom:1
}

.fl {
  float:left;
}
.fr {
  float:right;
}
li {
  list-style: none;
}
a{
  text-decoration: none;
}